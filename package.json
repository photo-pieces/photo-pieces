{
  "name": "photo-pieces",
  "version": "1.34.0",
  "description": "photo-pieces game built in React",
  "keywords": [
    "photo-pieces",
    "react",
    "game",
    "web-game",
    "pwa"
  ],
  "main": "src/index.js",
  "dependencies": {
    "atob": "^2.1.2",
    "axios": "^0.19.0",
    "chrome-aws-lambda": "^1.17.1",
    "puppeteer-core": "^1.18.0",
    "react": "^16.8.6",
    "react-dnd": "^7.6.1",
    "react-dnd-touch-backend": "^9.0.0",
    "react-dom": "^16.8.6",
    "react-ga": "^2.5.7",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "workbox-cli": "^4.3.1"
  },
  "scripts": {
    "release": "release-it --config=tools/scripts/.release-it.json --verbose",
    "prerelease": "git-branch-is master",
    "format": "npx prettier  api/**/*.js* --write & npx prettier  src/**/*.js* --write",
    "now-build": "yarn build",
    "now-dev": "now dev",
    "start": "react-scripts start",
    "build": "react-scripts build && npm run generate-sw",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "node tools/scripts/deploy.js",
    "generate-sw": "npx workbox generateSW tools/scripts/workbox-config.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "cross-env": "^5.2.0",
    "dotenv": "^8.0.0",
    "execa": "^1.0.0",
    "git-branch-is": "^2.1.0",
    "husky": "^2.4.1",
    "node-sass": "^4.12.0",
    "release-it": "^12.3.0"
  }
}
