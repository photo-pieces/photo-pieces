{
  "name": "photo-pieces",
  "version": "1.28.0",
  "description": "photo-pieces game built in React",
  "keywords": [
    "photo-pieces",
    "react",
    "game",
    "web-game",
    "pwa"
  ],
  "main": "src/index.js",
  "dependencies": {
    "atob": "^2.1.2",
    "chrome-aws-lambda": "^1.12.0",
    "puppeteer-core": "^1.12.2",
    "react": "^16.8.3",
    "react-dnd": "^7.0.2",
    "react-dnd-touch-backend": "^0.8.0",
    "react-dom": "^16.8.3",
    "react-ga": "^2.5.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.5",
    "workbox-cli": "^3.6.3"
  },
  "scripts": {
    "release": "release-it --config=tools/scripts/.release-it.json --verbose",
    "prerelease": "git-branch-is master",
    "format": "npx prettier  api/**/*.js* --write & npx prettier  src/**/*.js* --write",
    "now-build": "yarn build",
    "now-dev": "npx dev-server -w",
    "start": "react-scripts start",
    "build": "react-scripts build && npm run generate-sw",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "node tools/scripts/deploy.js",
    "generate-sw": "npx workbox generateSW tools/scripts/workbox-config.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@iridium/dev-server": "^0.0.2",
    "cross-env": "^5.2.0",
    "dotenv": "^6.2.0",
    "execa": "^1.0.0",
    "git-branch-is": "^2.1.0",
    "husky": "^1.3.1",
    "node-sass": "^4.11.0",
    "release-it": "^10.1.0"
  }
}
